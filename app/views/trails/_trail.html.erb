<div id="<%= dom_id trail %>">
  <p>
    <strong>User:</strong>
    <%= trail.user_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= trail.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= trail.description %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= trail.category %>
  </p>
<ul>
<% trail.video_contents.each do |video| %>
<li><%=video.title %> <%= link_to "see video","https://www.youtube.com/watch?v=#{video.video_id}" %>
<% end %>
</ul>
</div>
